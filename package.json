{
  "name": "stagingfy-backend",
  "version": "1.0.0",
  "description": "Backend API for Stagingfy - Virtual Staging SaaS Platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [
    "virtual-staging",
    "saas",
    "api",
    "real-estate"
  ],
  "author": "Stagingfy Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "tsc-alias": "^1.8.8",
    "@aws-sdk/client-s3": "^3.892.0",
    "@aws-sdk/s3-request-presigner": "^3.892.0",
    "@logtail/node": "^0.5.6",
    "@neondatabase/serverless": "^1.0.1",
    "@sentry/node": "^10.12.0",
    "better-auth": "^1.3.12",
    "bullmq": "^5.58.5",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "sharp": "^0.34.4",
    "stripe": "^18.5.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.12",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.5.2",
    "@types/sharp": "^0.32.0",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.35.0",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.3",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
